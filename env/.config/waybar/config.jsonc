{
  "layer": "top",
  "position": "top",
  "height": 40,

  "modules-left": [
    "sway/workspaces",
    "sway/window"
  ],
  "sway/workspaces": {
    "format": " {icon} ",
  },
 "sway/window": {
    "format": "{app_id}",
    "separate-outputs": true,
    "icon":true
  },
  // "modules-left": [
  //   "hyprland/workspaces",
  //   "hyprland/window"
  // ],
  //   "hyprland/workspaces": {
  //     "format": " {icon} ",
  //   },
  //     "hyprland/window": {
  //         "format": "{class}",
  //         "separate-outputs": true,
  //         "icon":true,
  //         "rewrite": {
  //             "zen-beta": "Zen browser",
  //             "com.mitchellh.ghostty": "Ghostty",
  //             "org.gnome.Nautilus": "Nautilus",
  //         }
  //     },
  "modules-center": ["clock"],
  "modules-right": [
    "battery",
    "pulseaudio",
    "backlight",
    "temperature",
    "cpu",
    "network"
  ],
  "network": {
    "format-wifi": "{icon} {essid}",
    "format-ethernet": "󰈀",
    "format-icons": ["󰤯 ", "󰤟 ", "󰤢 ", "󰤢 ", "󰤨 "],
    "format-disconnected": "󰪎",
    "interval": 5,
    "tooltip-format-wifi": " {bandwidthDownBytes} |  {bandwidthUpBytes} | {essid}",
    "tooltip-format-disconnected": "Disconnected",
    "on-click": "exec ~/.config/rofi/wifi/wifi.sh",
    "on-click-right": "ghostty -e nmtui"
  },
  "network#speed": {
    "interval": 1,
    "format": "{ifname}%%",
    "format-wifi": " {bandwidthDownBytes}",
    "format-ethernet": " {bandwidthDownBytes}  {bandwidthUpBytes} ",
    "format-disconnected": "󰌙",
    "tooltip-format": "{ipaddr}",
    "format-linked": "󰈁 {ifname} (No IP)",
    "tooltip-format-wifi": "{essid} {icon} {signalStrength}%",
    "tooltip-format-ethernet": "{ifname} 󰌘",
    "tooltip-format-disconnected": "󰌙 Disconnected",
    "max-length": 22,
    "min-length": 20,
    "format-icons": ["󰤯", "󰤟", "󰤢", "󰤥", "󰤨"]
  },
  "disk": {
    "format": "   {free}",
    "interval": 20
  },
  "cpu": {
    "interval": 1,
    "format": "<span font='Font Awesome 6 Free-Solid'>\uf2db</span>  {usage}%",
    "on-click": "ghostty -e btop"
  },
  "temperature": {
    "tooltip":false,
    "thermal-zone": 2,
    "hwmon-path": "/sys/class/hwmon/hwmon1/temp1_input",
    "critical-threshold": 70,
    "format": " {temperatureC}°C",
    "format-critical": "{temperatureC}°C ",
  },
  "backlight": {
    "device": "intel_backlight",
    "scroll-step": 5,
    "format": "{icon}{percent}",
    "format-icons": ["󰃞 ", "󰃟 ", "󰃝 ", "󰃠 "],
    "tooltip": true,
    "tooltip-format": "{percent}%"
  },
  "memory": {
    "interval": 1,
    "format": "<span >\uf538</span>  {percentage}%",
    "on-click": "ghostty btop",
    "tooltip-format": "{used}/{total} GiB"
  },
  "pulseaudio": {
    "format": "<span >{icon}</span>  {volume}",
    "format-muted": "<span >\uf6a9</span>",
    "format-icons": {
      "headphone": "\uf025",
      "default": ["\uf028"]
    },
    "tooltip-format": "{volume}% {desc}",
    "on-click": "pavucontrol"
  },
  "battery": {
    "interval": 1,
    "states": {
      "good": 99,
      "warning": 30,
      "critical": 20
    },
    "format-icons": ["󰂎","󰁺","󰁻","󰁽","󰁾","󰁿","󰂀","󰂁","󰂂","󰁹"],
    "format": "{icon} {capacity}%",
    "format-critical": "{icon} {capacity}%!!",
    "format-warning": "{icon} {capacity}%",
    "format-full": "{icon} {capacity}%",
    "format-charging": "󰂅 {capacity}%",
    "format-charging-warning": "󰢝 {capacity}%",
    "format-charging-critical": "󰢜 {capacity}%",
    "format-plugged": " 󰂅  {capacity}%",
    "format-alt": " 󱧥  {time}",
    "tooltip": false
  },

  "clock": {
    "format": "<span font='Font Awesome 6 free'>\uf017 </span>{:%a %d %H:%M}",

    "tooltip": true,
    "tooltip-format": "<tt>{calendar}</tt>"
  }
}
